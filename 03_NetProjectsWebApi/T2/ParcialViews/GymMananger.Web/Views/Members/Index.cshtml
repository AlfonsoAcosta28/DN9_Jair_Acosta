@model GymMananger.Web.Models.MemberListViewModel

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
    More text content 
*@
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeaderRight{
    <form class="d-flex mx-auto">
        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success bg-dark" type="submit">Search</button>
    </form>
}


<div class="container">
     <h1 class="mt-5">MEMBERS</h1>
     <p>New members of week: @Model.NewMembersCount</p>
    @await Html.PartialAsync("~/Views/Shared/_MemberList.cshtml", Model.Members)
 </div>
   