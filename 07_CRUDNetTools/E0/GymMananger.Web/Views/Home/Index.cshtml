@page
@model GymManager.Web.Views.Home.IndexModel
@{
}
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<GymManager.Web.Views.Home.IndexModel> localizar

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">@localizar.GetString("Welcom")</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
        <div class="btn-group me-2">
            <button type="button" class="btn btn-sm btn-outline-secondary">@localizar.GetString("Share")</button>
            <button type="button" class="btn btn-sm btn-outline-secondary">@localizar.GetString("Export")</button>
        </div>
        <button type="button" class="btn btn-sm btn-outline-secondary dropdown-toggle d-flex align-items-center gap-1">
            <svg class="bi"><use xlink:href="#calendar3" /></svg>
            @localizar.GetString("This week")
        </button>
    </div>
</div>
<div class="container">
   
    <canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>
</div>

@section Scripts{
  
}